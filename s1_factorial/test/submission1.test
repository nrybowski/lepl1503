    facto: |-
        int factorial(int i)
        {
            int b = 1;
            if (i == 0)
            {
                return 1;
            }
            else
            {
                int a;
                for(a = 1; a <= i; a++)
                {
                    b = b*a;
                }
            }
            return b;
        }
